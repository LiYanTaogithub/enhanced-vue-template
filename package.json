    "china-area-data": "^5.0.1",
    "clipboard": "2.0.4",
    "core-js": "^3.6.5",
    "echarts": "^4.2.1",
    "element-china-area-data": "^5.0.2",
    "element-ui": "2.13.2",
    "file-saver": "2.0.1",
    "fuse.js": "3.4.4",
    "html2canvas": "^1.0.0-rc.7",
    "js-beautify": "1.10.2",
    "js-cookie": "2.2.0",
    "jsencrypt": "3.0.0-rc.1",
    "jspdf": "^2.1.1",
    "latest": "^0.2.0",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "screenfull": "4.2.0",
    "sockjs-client": "^1.5.0",
    "sortablejs": "1.8.4",
    "stompjs": "^2.3.3",
    "vue": "2.6.10",
    "vue-count-to": "1.0.13",
    "vue-cropper": "0.4.9",
    "vue-quill-editor": "3.0.6",
    "vue-router": "^3.4.9",
    "vue-splitpane": "1.0.4",
    "vuedraggable": "2.20.0",
    "vuex": "3.1.0"
