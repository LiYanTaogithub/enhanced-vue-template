    "@vue/cli-service": "4.4.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "9.5.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "23.6.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "chalk": "2.4.2",
    "chokidar": "2.1.5",
    "connect": "3.6.6",
    "eslint": "6.7.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "6.2.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "mockjs": "1.0.1-beta3",
    "node-sass": "4.14.1",
    "plop": "2.3.0",
    "prettier": "^2.1.1",
